  },
  "homepage": "https://github.com/leobrossault/Three-Starter#readme",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.20",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "domready": "^1.0.8",
    "raf": "^3.1.0",
    "three": "^0.73.0",
    "watchify": "^3.6.0"
  }
